
<div class="contenedorFormulario">
        <div class="formulario">
            <div class="ingreso">
                <h2>Inicio de sesión</h2>
                <form action="/" method="POST" onsubmit = localStorage.setItem("enabled","true")>
                    <input type="text" placeholder=" Usuario" name="usuario" >
                    <input type="password" placeholder=" Contraseña" name="contrasenia" autocomplete="on">
                    <input type="submit" class="entradaUsuario">
                    {{#if accesoDenegado}} 
                    <p class="errorIngreso" data-enabled={{this.enabled}}>{{this.accesoDenegado}}</p>
                    {{/if}}
                </form>
                <div>
                    <p>¿No cuentas con un usuario?</p><a href="/register">Registrate aqui</a>
                </div>
            </div>
                
            <div class="ingresoDiseño">
